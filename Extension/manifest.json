{
    "manifest_version": 2,
    "name": "PreMiD Wrapper",
    "version": "0.0.1",
    "icons": {
        "16": "icons/16x16.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "webNavigation"
    ],
    "optional_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "Pages/pages.js",
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}