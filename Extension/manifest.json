{
    "manifest_version": 2,
    "name": "PreMiD Wrapper",
    "version": "0.0.1",
    "permissions": [
        "storage",
        "activeTab",
        "webNavigation"
    ],
    "optional_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "Pages/pages.js",
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}